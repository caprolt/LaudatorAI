# API Configuration
API_V1_STR=/api/v1
PROJECT_NAME=LaudatorAI

# CORS Configuration - Production
BACKEND_CORS_ORIGINS=["https://laudator-ai.vercel.app","https://laudator-ai-git-main-caprolt.vercel.app","https://laudator-ai-caprolt.vercel.app"]

# Database Configuration
# These will be set by Railway automatically
# POSTGRES_SERVER=your-railway-postgres-host
# POSTGRES_USER=your-railway-postgres-user
# POSTGRES_PASSWORD=your-railway-postgres-password
# POSTGRES_DB=your-railway-postgres-db
# DATABASE_URL=your-railway-postgres-url

# Redis Configuration
# This will be set by Railway automatically
# REDIS_URL=your-railway-redis-url

# File Storage Configuration
# These will be set by Railway automatically
# MINIO_ENDPOINT=your-minio-endpoint
# MINIO_ACCESS_KEY=your-minio-access-key
# MINIO_SECRET_KEY=your-minio-secret-key
# MINIO_BUCKET_NAME=laudatorai
# MINIO_SECURE=true

# LLM Configuration
OPENAI_API_KEY=your_openai_api_key_here
LLM_PROVIDER=openai

# Sentry Configuration
SENTRY_DSN=your_sentry_dsn_here

# Environment
ENVIRONMENT=production
DEBUG=false
