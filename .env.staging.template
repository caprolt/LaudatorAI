# Staging Environment Variables Template
# Copy this file to .env.staging and fill in the actual values

# Database Configuration
POSTGRES_DB=laudatorai_staging
POSTGRES_USER=postgres
POSTGRES_PASSWORD=staging_password

# Redis Configuration
REDIS_URL=redis://redis:6379/0

# MinIO Configuration
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=staging_password
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=staging_password
MINIO_BUCKET_NAME=laudatorai_staging

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Application Configuration
ENVIRONMENT=staging
LOG_LEVEL=DEBUG
NEXT_PUBLIC_API_URL=http://localhost:8001

# Grafana Configuration
GRAFANA_PASSWORD=admin
